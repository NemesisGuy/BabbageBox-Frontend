{
  "name": "babbagebox-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .vue,.ts,.js",
    "lint:fix": "eslint src --ext .vue,.ts,.js --fix",
